{
  "name": "asphalt-tracker",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node --import tsx/esm server/index.ts",
    "dev:tsx": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --target=node18",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:dev": "cross-env NODE_ENV=development node --import tsx/esm server/index.ts",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "health": "curl -f http://localhost:5000/health || exit 1",
    "mock:populate": "curl -s -X POST http://localhost:5000/api/mock/populate/quick",
    "mock:simulate": "curl -s -X POST http://localhost:5000/api/mock/simulation/start",
    "mock:status": "curl -s http://localhost:5000/api/mock/status",
    "streaming:health": "curl -s http://localhost:5000/api/streams/health",
    "streaming:status": "curl -s http://localhost:5000/api/streams/status",
    "streaming:init": "curl -s -X POST http://localhost:5000/api/streams/initialize",
    "restack:check": "curl -s http://localhost:5000/api/restack/status",
    "restack:health": "curl -f http://localhost:5000/health | grep '@restackio/ai' || exit 1",
    "restack:validate": "node -e 'console.log(\"âœ… Restack AI dependency found:\", require(\"./package.json\").dependencies[\"@restackio/ai\"])'",
    "test:restack": "npm run restack:validate && npm run health && npm run restack:check",
    "services": "cross-env NODE_ENV=development tsx src/services.ts",
    "services:build": "esbuild src/services.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --target=node18",
    "services:start": "cross-env NODE_ENV=production node dist/services.js",
    "dev:all": "node start-development.cjs",
    "dev:all:concurrent": "concurrently \"npm run dev\" \"npm run services\"",
    "build:all": "npm run build && npm run services:build",
    "start:all": "node start-production.cjs",
    "start:all:concurrent": "concurrently \"npm run start\" \"npm run services:start\"",
    "start:app": "npm run start",
    "start:services-only": "npm run services:start",
    "start:windows": "start-all.bat",
    "start:powershell": "powershell -ExecutionPolicy Bypass -File start-all.ps1",
    "infrastructure:start": "docker-compose -f docker-compose.infrastructure.yml up -d",
    "infrastructure:stop": "docker-compose -f docker-compose.infrastructure.yml down",
    "infrastructure:restart": "docker-compose -f docker-compose.infrastructure.yml restart",
    "infrastructure:logs": "docker-compose -f docker-compose.infrastructure.yml logs -f",
    "infrastructure:status": "docker-compose -f docker-compose.infrastructure.yml ps",
    "infrastructure:setup": "powershell -ExecutionPolicy Bypass -File start-infrastructure.ps1",
    "infrastructure:setup:windows": "start-infrastructure.bat"
  },
  "dependencies": {
    "@restackio/ai": "^0.0.126",
    "@hookform/resolvers": "^3.10.0",
    "@neondatabase/serverless": "^0.10.4",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-aspect-ratio": "^1.1.3",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-collapsible": "^1.1.4",
    "@radix-ui/react-context-menu": "^2.2.7",
    "@radix-ui/react-dialog": "^1.1.7",
